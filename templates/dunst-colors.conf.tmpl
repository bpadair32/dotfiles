# Dunst Colors - Theme: ${THEME_NAME}
# Generated by theme-switch
# Include this from dunstrc: include = colors.conf

[urgency_low]
background = "${SURFACE_CONTAINER}"
foreground = "${ON_SURFACE_VARIANT}"
frame_color = "${OUTLINE_VARIANT}"
timeout = 10

[urgency_normal]
background = "${SURFACE_CONTAINER_HIGH}"
foreground = "${ON_SURFACE}"
frame_color = "${OUTLINE}"
timeout = 10

[urgency_critical]
background = "${ERROR_CONTAINER}"
foreground = "${ON_ERROR_CONTAINER}"
frame_color = "${ERROR}"
timeout = 0
